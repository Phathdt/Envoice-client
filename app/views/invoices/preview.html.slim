                td[colspan="5" valign="middle"]
                  span[style="line-height:17px;"]
                    = @invoice.company.address
              tr
                td[width="100" valign="top" nowrap="nowrap"]
                  | Điện thoại
                td[width="5" valign="top"]
                  | :
                td[colspan="5" valign="middle"]
                  span[style="line-height:17px;"]
